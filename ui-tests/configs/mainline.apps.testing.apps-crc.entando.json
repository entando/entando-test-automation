{
  "baseUrl": "http://entando.apps-crc.testing.apps.mainline.eng-entando.com",
  "basePath": "/app-builder",
  "restAPI": "http://entando.apps-crc.testing.apps.mainline.eng-entando.com/entando-de-app/api/",
  "env": {
    "auth_base_url": "http://entando.apps-crc.testing.apps.mainline.eng-entando.com/auth",
    "auth_realm": "entando",
    "auth_client_id": "entando-web"
  },
  "testFiles": "**/*.spec.js",
  "video": false,
  "chromeWebSecurity": false,
  "viewportWidth": 1200,
  "viewportHeight": 720,
  "retries": {
    "runMode": 2,
    "openMode": 0
  },
  "reporter": "cypress-mochawesome-reporter",
  "reporterOptions": {
    "reportTitle": "App-builder functional tests",
    "reportPageTitle": "App-builder functional tests",
    "reportDir": "cypress/results",
    "charts": true
  }
}